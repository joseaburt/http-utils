{
  "name": "@joseaburt/http-utils",
  "version": "0.1.3",
  "license": "MIT",
  "description": "This is a package to abstract all boilerplate configurations for a production http client library. Axios is used as base provider.",
  "author": {
    "name": "Jose Aburto",
    "email": "pino0071@gmail.com",
    "url": "https://www.linkedin.com/in/jose-aburto/"
  },
  "main": "dist/index.js",
  "module": "dist/del-http-utils.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "homepage": "https://github.com/joseaburt/http-utils#readme",
  "bugs": {
    "url": "https://github.com/joseaburt/http-utils/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joseaburt/http-utils.git"
  },
  "keywords": [
    "axios",
    "http",
    "utils",
    "rest",
    "abortable",
    "http-error"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 200,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/del-http-utils.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/del-http-utils.esm.js",
      "limit": "10 KB"
    }
  ],
  "peerDependencies": {
    "axios": "^1.3.5",
    "lodash": "^4.17.21",
    "lodash-es": "4.17.21"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^10.0.1",
    "@tsconfig/recommended": "^1.0.3",
    "axios": "^1.3.5",
    "dts-cli": "^2.0.3",
    "husky": "^8.0.3",
    "@types/lodash": "^4.14.200",
    "lodash": "^4.17.21",
    "lodash-es": "4.17.21",
    "size-limit": "^10.0.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  }
}